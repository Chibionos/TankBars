package-as: TankBars

enable-nolib-creation: no

ignore:
  - .github
  - .git
  - .gitignore
  - .pkgmeta
  - README.md
  - LICENSE
  - CHANGELOG.md
  - "*.zip"
  - .claude
  - docs

move-folders:
  TankBars: TankBars

manual-changelog:
  filename: CHANGELOG.md
  markup-type: markdown

license-output: LICENSE